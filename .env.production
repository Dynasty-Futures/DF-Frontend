# Backend API URL (production - update when ECS is deployed)
VITE_API_URL=https://dynasty-futures-alb-prod-124877638.us-east-1.elb.amazonaws.com # https://api.dynastyfuturesdyn.com <--- UPDATE TO THIS ONCE WE HAVE A DOMAIN

# Use mock data (always false in production)
VITE_USE_MOCK_DATA=false

# Disable debug logging in production
VITE_DEBUG_MODE=false
